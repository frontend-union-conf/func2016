@define-mixin headerFont {
  color: #111;
  font-size: 1.2rem;
  font-weight: 700;
  letter-spacing: 0.18em;
  text-transform: uppercase;
  text-decoration: none;
}

@define-mixin containerMain {
  max-width: var(--maxInnerWidth);
  margin-left: auto;
  margin-right: auto;
}

@define-mixin fontSpacing {
  font-weight: 700;
  letter-spacing: 0.18em;
  text-transform: uppercase;
}
